{
   "browser_action": {
      "default_icon": "img/passo.png",
      "default_popup": "popup.html",
      "default_title": "Pass-Roll"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/style.css" ],
      "js": [ "js/content.js", "js/jquery.1.8.3.min.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Rotate your passwords to make them more complex!",
   "icons": {
      "128": "img/passo.png",
      "16": "img/passo.png",
      "48": "img/passo.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Pass-Roll",
   "options_page": "options.html",
   "permissions": [ "storage", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3",
   "web_accessible_resources": [ "img/*.png", "css/*.css" ]
}
